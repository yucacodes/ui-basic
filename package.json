{
  "name": "@yucacodes/ui",
  "private": false,
  "version": "0.0.22",
  "type": "module",
  "qwik": "./dist/main.qwik.mjs",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build:docs": "qwik build",
    "build:lib.qwik": "tsc --p ./tsconfig.lib.json  && VITE_BUILD_MODE=lib-qwik vite build --mode lib",
    "build:lib.react": "tsc --p ./tsconfig.lib.json  && VITE_BUILD_MODE=lib-react vite build --mode lib",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview:docs": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.19",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/web": "^0.0.131",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "compression": "^1.7.4",
    "express": "4.18.2",
    "glob": "^10.3.10",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.3.2",
    "@builder.io/qwik-city": "^1.3.2",
    "@fontsource/ibm-plex-sans": "^5.0.18",
    "@fontsource/lato": "^5.0.18",
    "@qwikest/icons": "^0.0.13",
    "classnames": "^2.5.1",
    "highlight.js": "^11.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
